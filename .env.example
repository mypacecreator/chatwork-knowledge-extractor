# Chatwork API設定
CHATWORK_API_TOKEN=your_chatwork_api_token_here
CHATWORK_ROOM_ID=your_room_id_here

# Claude API設定
CLAUDE_API_KEY=your_claude_api_key_here

# 分析設定
# 分析対象の期間（省略時は全期間）
# - 数字: 過去N日間（例: 90）
# - 日付: 指定日以降（例: 2025-01-01）
EXTRACT_FROM=90
# 分析対象の最大件数（キャッシュから取り出す件数）
MAX_MESSAGES=500

# 出力設定
OUTPUT_DIR=./output
